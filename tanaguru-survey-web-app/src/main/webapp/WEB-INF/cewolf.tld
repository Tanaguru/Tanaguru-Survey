            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>onmouseover</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>onmouseup</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
