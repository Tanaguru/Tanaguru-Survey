<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">

    <description>This tag library is designed to describe charts of all kinds.</description>
    <tlib-version>1.1</tlib-version>
    <short-name>cewolf</short-name>
    <uri>http://cewolf.sourceforge.net/taglib/cewolf.tld</uri>
 
    <tag>
        <description>
		The plot tag is nested inside a complex chart to define a nested or layered plot instance.
        </description>
        <name>plot</name>
        <tag-class>de.laures.cewolf.taglib.tags.PlotTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>
		The type of the plot. One of xyarea, xyline, xyshapesandlines, scatter, xyverticalbar, step,
		candlestick, highlow, signal, verticalbar, area, line, shapesandlines, spline, stackedxyarea, stackedxyarea2
            </description>
            <name>type</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Title of the plot's x-axis. This attribute is only used for horizontally combined charts.
            </description>
            <name>xaxislabel</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Title of the plot's y-axis. This attribute is only used for vertically combined charts.
            </description>
            <name>yaxislabel</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Boolean value to indicate whether the label values of a numeric domain (X) axis
		should have integer values only, or if decimal values are OK. Default: false.
		&lt;BR&gt;This attribute is new in cewolf version 1.1.
            </description>
            <name>xaxisinteger</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Boolean value to indicate whether the label values of a numeric range (Y) axis
		should have integer values only, or if decimal values are OK. Default: false.
		&lt;BR&gt;This attribute is new in cewolf version 1.1.
            </description>
            <name>yaxisinteger</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
  
    <tag>
        <description>
		This tag is a specialized HTML img tag. It renders the chart
		which has been specified by a &amp;lt;chart&gt; tag in the same page.
		&lt;BR&gt;optional childs: &amp;lt;map&gt;
        </description>
        <name>img</name>
        <tag-class>de.laures.cewolf.taglib.tags.ChartImgTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>
		Path to rendering servlet. Any path that starts with a '/' is assumed to refer to a resource
		relative to web application root. Otherwise, the path is assumed to refer to resource relative
		to the current page directory. 
            </description>
            <name>renderer</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		The id of the chart to render. 
            </description>
            <name>chartid</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Width of the rendered chart image in pixel. 	
            </description>
            <name>width</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Height of the rendered chart image in pixel. 	
            </description>
            <name>height</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		The MIME type of the image. Currently image/png (default), image/jpeg and image/svg+xml are supported. 	
            </description>
            <name>mime</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 
            </description>
            <name>alt</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		How long to store the image in the session; default is 300 (seconds).
            </description>
            <name>timeout</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 
            </description>
            <name>hspace</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>vspace</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>align</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>border</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>ismap</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>longdesc</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
		Corresponds to appropriate HMTL &amp;lt;img> attribute. 	
            </description>
            <name>usemap</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
